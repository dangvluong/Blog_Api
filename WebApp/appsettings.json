{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"       
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log.txt"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ApiServer": "https://localhost:7207"
}
